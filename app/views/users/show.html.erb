        </svg>
      </div>
      <p><%= @user.profile.equipment %></p>
    </div>
  </div>
  <br>
  <p>Last matches:</p>
  <div class="last-matches">
    <% @lastparticipations.each do |participation| %>
      <% matchparticipations = participation.match.participations %>
      <% winners = []%>
      <% losers = []%>
      <% matchparticipations.each do |part|%>
        <% if part.outcome == "won" %>
          <% winners << part %>
        <% else %>
          <% losers << part %>
        <% end %>
      <% end %>
      <% if participation.outcome == "won" %>
        <div class="last-match won">
          <div class="stats">
            <div class="winners">
              <% if winners.length == 1 %>
                <div class="matchavatars-1">
                  <% winners.each do |winner| %>
                    <%= cl_image_tag winner.user.photo.key, class: "avatar-bordered-small" %>
                  <% end %>
                </div>
              <% else %>
               <div class="matchavatars-2">
                  <% winners.each do |winner| %>
                    <%= cl_image_tag winner.user.photo.key, class: "avatar-bordered-small" %>
                  <% end %>
                </div>
              <% end %>
              <% if participation.match.score.score_a > participation.match.score.score_b %>
                <p><%= participation.match.score.score_a %></p>
              <% else %>
                <p><%= participation.match.score.score_b %></p>
              <% end %>
            </div>
            <div class="losers">
              <% if losers.length == 1 %>
                <div class="matchavatars-1">
                  <% losers.each do |loser| %>
                    <%= cl_image_tag loser.user.photo.key, class: "avatar-bordered-small" %>
                  <% end %>
                </div>
              <% else %>
               <div class="matchavatars-2">
                  <% losers.each do |loser| %>
                    <%= cl_image_tag loser.user.photo.key, class: "avatar-bordered-small" %>
                  <% end %>
                </div>
              <% end %>
              <% if participation.match.score.score_a < participation.match.score.score_b %>
                <p><%= participation.match.score.score_a %></p>
              <% else %>
                <p><%= participation.match.score.score_b %></p>
              <% end %>
            </div>
          </div>
          <svg width="20" height="25" viewBox="0 0 57 84" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M2.76222 38.6424C4.29593 55.7023 14.6726 62.5565 29.728 62.5474C44.7834 62.5382 54.8227 55.8804 54.8288 38.6109C53.2951 21.5509 42.86 1.31001 27.9925 3.60201C13.125 5.89402 1.22851 21.5825 2.76222 38.6424Z" fill="#F5455C"/>
            <rect width="9.82948" height="57.5464" rx="4.91474" transform="matrix(-0.999759 0.0219325 0.0205249 0.999789 33.9253 26.2073)" fill="#F5455C"/>
            <path d="M55.8903 33.6167C55.8741 59.7671 44.3717 56.8554 29.1292 56.8453C14.7829 56.8358 3.93606 60.023 2.60322 35.3108C2.61377 18.3024 13.9851 1.77913 28.3314 1.78864C42.6778 1.79815 55.9008 16.6083 55.8903 33.6167Z" fill="#5CA39C"/>
